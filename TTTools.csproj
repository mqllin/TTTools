    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <BaseOutputPath>bin\build</BaseOutputPath>
    <PlatformTarget>x86</PlatformTarget>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
